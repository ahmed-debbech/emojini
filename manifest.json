{
    "manifest_version": 2,
    "name": "Emojini",
    "version": "0.1",
    "description": "Key bind your emojis for easy writting",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs", "activeTab"]
  }